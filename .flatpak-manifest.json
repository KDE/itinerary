                        "url-template": "https://download.kde.org/stable/release-service/$version/src/kpkpass-$version.tar.xz"
                    }
                }
            ],
            "cleanup": [
                "/include",
                "/lib/cmake",
                "/mkspecs",
                "/share/qlogging-categories6"
            ]
        },
        {
            "name": "kmime",
            "config-opts": [
                "-DBUILD_TESTING=OFF",
                "-DCMAKE_BUILD_TYPE=RelWithDebInfo"
            ],
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "sources": [
                {
                    "type": "git",
                    "url": "https://invent.kde.org/pim/kmime",
                    "branch": "release/25.12"
                }
            ],
            "cleanup": [
                "/include",
                "/lib/cmake",
                "/mkspecs",
                "/share/qlogging-categories6"
            ]
        },
        {
            "name": "zxing",
            "config-opts": [
                "-DBUILD_DEPENDENCIES=LOCAL",
