{
    "id": "org.kde.itinerary",
    "branch": "master",
    "runtime": "org.kde.Platform",
    "runtime-version": "6.9",
    "sdk": "org.kde.Sdk",
    "command": "itinerary",
    "tags": [
        "nightly"
    ],
    "desktop-file-name-suffix": " (Nightly)",
    "finish-args": [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri",
        "--device=all",
        "--share=network",
        "--talk-name=org.freedesktop.secrets",
        "--talk-name=org.kde.kwalletd5",
        "--talk-name=org.kde.kwalletd6"
    ],
    "modules": [
        {
            "name": "poppler",
            "config-opts": [
                "-DENABLE_TESTING=OFF",
                "-DENABLE_UNSTABLE_API_ABI_HEADERS=ON",
                "-DENABLE_BOOST=OFF",
                "-DENABLE_CPP=OFF",
                "-DENABLE_GLIB=OFF",
                "-DENABLE_GOBJECT_INTROSPECTION=OFF",
                "-DENABLE_LIBCURL=OFF",
                "-DENABLE_UTILS=OFF",
                "-DENABLE_QT5=OFF",
                "-DENABLE_QT6=OFF",
                "-DENABLE_SPLASH=OFF",
                "-DWITH_Cairo=OFF"
            ],
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "sources": [
                {
                    "type": "git",
                    "url": "https://invent.kde.org/mirrors/poppler.git",
                    "branch": "master"
                }
            ],
            "cleanup": [
                "/include",
                "/lib/pkgconfig"
            ]
        },
        {
            "name": "qtkeychain",
            "buildsystem": "cmake-ninja",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://github.com/frankosterfeld/qtkeychain/archive/0.15.0.tar.gz",
                    "sha256": "f4254dc8f0933b06d90672d683eab08ef770acd8336e44dfa030ce041dc2ca22",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 4138,
                        "stable-only": true,
                        "url-template": "https://github.com/frankosterfeld/qtkeychain/archive/$version.tar.gz"
                    }
                }
            ],
            "config-opts": [
                "-DCMAKE_INSTALL_LIBDIR=/app/lib",
                "-DLIB_INSTALL_DIR=/app/lib",
                "-DBUILD_TRANSLATIONS=NO",
                "-DBUILD_WITH_QT6=ON"
            ],
            "cleanup": [
                "/include",
                "/lib/cmake",
                "/mkspecs"
            ]
        },
        {
            "name": "libphonenumer",
            "buildsystem": "cmake-ninja",
            "subdir": "cpp",
            "config-opts": [
                "-DUSE_BOOST=OFF",
                "-DBUILD_TESTING=OFF",
                "-DREGENERATE_METADATA=OFF",
                "-DBUILD_STATIC_LIB=ON",
                "-DBUILD_SHARED_LIBS=OFF",
                "-DUSE_PROTOBUF_LITE=ON",
                "-DBUILD_GEOCODER=OFF",
                "-DFETCHCONTENT_FULLY_DISCONNECTED=ON",
                "-DCMAKE_POSITION_INDEPENDENT_CODE=ON"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://github.com/google/libphonenumber/archive/refs/tags/v9.0.13.tar.gz",
                    "sha256": "46400323d2df4fdefd57bc46a34111dc2c4612da62ecd0cedebff5ad94e49b0b",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 89344,
                        "stable-only": true,
                        "url-template": "https://github.com/google/libphonenumber/archive/refs/tags/v$version.tar.gz"
                    }
                }
            ],
            "cleanup": [
                "/include",
                "/lib/cmake"
            ],
            "modules": [
                {
                    "name": "abseil-cpp",
                    "buildsystem": "cmake-ninja",
                    "config-opts": [
                        "-DCMAKE_BUILD_TYPE=Release",
                        "-DABSL_PROPAGATE_CXX_STD=ON",
                        "-DBUILD_SHARED_LIBS=OFF",
                        "-DCMAKE_POSITION_INDEPENDENT_CODE=ON"
                    ],
                    "sources": [
                        {
                            "type": "archive",
                            "url": "https://github.com/abseil/abseil-cpp/archive/refs/tags/20250814.0.tar.gz",
                            "sha256": "9b2b72d4e8367c0b843fa2bcfa2b08debbe3cee34f7aaa27de55a6cbb3e843db",
                            "x-checker-data": {
                                "type": "anitya",
                                "project-id": 115295,
                                "stable-only": true,
                                "url-template": "https://github.com/abseil/abseil-cpp/archive/refs/tags/$version.tar.gz"
                            }
                        }
                    ],
                    "cleanup": [
                        "/include",
                        "/lib/*.a",
                        "/lib/cmake",
                        "/lib/pkgconfig"
                    ]
                },
                {
                    "name": "protobuf",
                    "buildsystem": "cmake-ninja",
                    "config-opts": [
                        "-Dprotobuf_BUILD_TESTS=OFF",
                        "-Dprotobuf_LOCAL_DEPENDENCIES_ONLY=ON",
                        "-DBUILD_SHARED_LIBS=OFF",
                        "-DCMAKE_POSITION_INDEPENDENT_CODE=ON"
                    ],
                    "sources": [
                        {
                            "type": "archive",
                            "url": "https://github.com/protocolbuffers/protobuf/archive/refs/tags/v32.1.tar.gz",
                            "sha256": "d2081ab9528292f7980ef2d88d2be472453eea4222141046ad4f660874d5f24e",
                            "x-checker-data": {
                                "type": "anitya",
                                "project-id": 3715,
                                "stable-only": true,
                                "url-template": "https://github.com/protocolbuffers/protobuf/archive/refs/tags/v$version.tar.gz"
                            }
                        }
                    ],
                    "cleanup": [
                        "/bin",
                        "/include",
                        "/lib/*.a",
                        "/lib/*.la",
                        "/lib/libprotoc*",
                        "/lib/libprotobuf-lite*",
                        "/lib/pkgconfig"
                    ]
                }
            ]
        },
        {
            "name": "kpkpass",
            "config-opts": [
                "-DBUILD_TESTING=OFF",
                "-DCMAKE_BUILD_TYPE=RelWithDebInfo"
            ],
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "sources": [
                {
                    "type": "git",
                    "url": "https://invent.kde.org/pim/kpkpass",
                    "branch": "master"
                }
            ],
            "cleanup": [
                "/include",
                "/lib/cmake",
                "/mkspecs",
                "/share/qlogging-categories6"
            ]
        },
        {
            "name": "kmime",
            "config-opts": [
                "-DBUILD_TESTING=OFF",
                "-DCMAKE_BUILD_TYPE=RelWithDebInfo"
            ],
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "sources": [
                {
                    "type": "git",
                    "url": "https://invent.kde.org/pim/kmime",
                    "branch": "master"
                }
            ],
            "cleanup": [
                "/include",
                "/lib/cmake",
                "/mkspecs",
                "/share/qlogging-categories6"
            ]
        },
        {
            "name": "zxing",
            "config-opts": [
                "-DBUILD_DEPENDENCIES=LOCAL",
                "-DBUILD_SHARED_LIBS=OFF",
                "-DBUILD_UNIT_TESTS=OFF",
                "-DBUILD_BLACKBOX_TESTS=OFF",
                "-DBUILD_EXAMPLES=OFF"
            ],
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "cleanup": [
                "/include",
                "*.pc",
                "/lib/*/cmake",
                "libZXing.so"
            ],
            "sources": [
                {
                    "type": "git",
                    "url": "https://github.com/zxing-cpp/zxing-cpp.git",
                    "branch": "master"
                }
            ]
        },
        {
            "name": "kitinerary",
            "config-opts": [
                "-DBUILD_TESTING=OFF",
                "-DBUILD_TOOLS=OFF"
            ],
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "sources": [
                {
                    "type": "git",
                    "url": "https://invent.kde.org/pim/kitinerary",
                    "branch": "master"
                }
            ],
            "cleanup": [
                "/include",
                "/lib/cmake",
                "/lib/libexec",
                "/mkspecs",
                "/share/qlogging-categories6"
            ]
        },
        {
            "name": "kirigami-addons",
            "config-opts": [
                "-DBUILD_TESTING=OFF"
            ],
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "sources": [
                {
                    "type": "git",
                    "url": "https://invent.kde.org/libraries/kirigami-addons",
                    "branch": "master"
                }
            ],
            "cleanup": [
                "/lib/cmake",
                "/share/kdevappwizard"
            ]
        },
        {
            "name": "kpublictransport",
            "config-opts": [
                "-DBUILD_TESTING=OFF",
                "-DBUILD_WITH_QT6=ON"
            ],
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "sources": [
                {
                    "type": "git",
                    "url": "https://invent.kde.org/libraries/kpublictransport",
                    "branch": "master"
                }
            ],
            "cleanup": [
                "/include",
                "/lib/cmake",
                "/mkspecs",
                "/share/qlogging-categories6"
            ]
        },
        {
            "name": "kopeninghours",
            "config-opts": [
                "-DBUILD_TESTING=OFF",
                "-DBUILD_WITH_QT6=ON"
            ],
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "sources": [
                {
                    "type": "git",
                    "url": "https://invent.kde.org/libraries/kopeninghours",
                    "branch": "master"
                }
            ],
            "cleanup": [
                "/include",
                "/lib/cmake",
                "/mkspecs",
                "/share/qlogging-categories6"
            ]
        },
        {
            "name": "kosmindoormap",
            "config-opts": [
                "-DBUILD_TESTING=OFF"
            ],
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "sources": [
                {
                    "type": "git",
                    "url": "https://invent.kde.org/libraries/kosmindoormap",
                    "branch": "master"
                }
            ],
            "cleanup": [
                "/include",
                "/lib/cmake",
                "/mkspecs",
                "/share/qlogging-categories6"
            ]
        },
        {
            "name": "khealthcertificate",
            "config-opts": [
                "-DBUILD_TESTING=OFF",
                "-DBUILD_WITH_QT6=ON"
            ],
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "sources": [ { "type": "git", "url": "https://invent.kde.org/pim/khealthcertificate.git", "branch": "master"} ],
            "cleanup": [
                "/include",
                "/lib/cmake",
                "/mkspecs",
                "/share/qlogging-categories6"
            ]
        },
        {
            "name": "libQuotient",
            "buildsystem": "cmake-ninja",
            "sources": [
                {
                    "type": "git",
                    "url": "https://github.com/quotient-im/libQuotient.git",
                    "branch": "dev",
                    "disable-submodules": true
                }
            ],
            "config-opts": [
                "-DCMAKE_BUILD_TYPE=Release",
                "-DQuotient_ENABLE_E2EE=ON",
                "-DBUILD_TESTING=OFF",
                "-DBUILD_WITH_QT6=ON"
            ],
            "cleanup": [
                "/include",
                "/lib/*.a",
                "/lib/cmake",
                "/lib/pkgconfig",
                "/share/ndk-modules"
            ],
            "modules": [
                {
                    "name": "olm",
                    "buildsystem": "cmake-ninja",
                    "builddir": true,
                    "config-opts": [
                        "-DOLM_TESTS=OFF",
                        "-DCMAKE_BUILD_TYPE=Release"
                    ],
                    "sources": [
                        {
                            "type": "archive",
                            "url": "https://gitlab.matrix.org/matrix-org/olm/-/archive/3.2.16/olm-3.2.16.tar.gz",
                            "sha256": "1e90f9891009965fd064be747616da46b232086fe270b77605ec9bda34272a68",
                            "x-checker-data": {
                                "type": "anitya",
                                "project-id": 29706,
                                "url-template": "https://gitlab.matrix.org/matrix-org/olm/-/archive/$version/olm-$version.tar.gz"
                            }
                        }
                    ],
                    "cleanup": [
                        "/include",
                        "/lib/cmake",
                        "/lib/pkgconfig"
                    ]
                }
            ]
        },
        {
            "name": "itinerary",
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "sources": [
                {
                    "type": "dir",
                    "path": "."
                }
            ],
            "config-opts": [
                "-DBUILD_TESTING=OFF",
                "-DCMAKE_DISABLE_FIND_PACKAGE_KF6KIO=ON",
                "-DCMAKE_DISABLE_FIND_PACKAGE_KF6FileMetaData=ON",
                "-DFLATPAK_BUILD=ON"
            ],
            "cleanup": [
                "/share/qlogging-categories6"
            ]
        }
    ]
}
