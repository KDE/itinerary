            ]
        },
        {
            "name": "libQuotient",
            "buildsystem": "cmake-ninja",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://github.com/quotient-im/libQuotient/archive/0.9.5.tar.gz",
                    "sha256": "24366dd59aca7b991756e45c3c8823c43fc6a8fdb161ea6dcce5b666c6300766",
                    "x-checker-data": {
