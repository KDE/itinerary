                    }
                }
            ]
        },
        {
            "name": "kitinerary",
            "config-opts": [
                "-DBUILD_TESTING=OFF",
                "-DBUILD_TOOLS=OFF"
            ],
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "sources": [
                {
                    "type": "git",
                    "url": "https://invent.kde.org/pim/kitinerary",
                    "branch": "release/25.12"
                }
            ],
            "cleanup": [
                "/include",
                "/lib/cmake",
                "/lib/libexec",
                "/mkspecs",
                "/share/qlogging-categories6"
            ]
        },
        {
            "name": "kirigami-addons",
            "config-opts": [
                "-DBUILD_TESTING=OFF"
            ],
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "sources": [
                {
                    "type": "archive",
