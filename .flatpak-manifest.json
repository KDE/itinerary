                    }
                }
            ],
            "config-opts": [
                "-DCMAKE_BUILD_TYPE=Release",
                "-DQuotient_ENABLE_E2EE=ON",
                "-DBUILD_TESTING=OFF",
                "-DBUILD_WITH_QT6=ON"
            ],
            "cleanup": [
                "/include",
                "/lib/*.a",
                "/lib/cmake",
                "/lib/pkgconfig",
                "/share/ndk-modules"
            ],
            "modules": [
                {
                    "name": "olm",
                    "buildsystem": "cmake-ninja",
                    "builddir": true,
                    "config-opts": [
                        "-DOLM_TESTS=OFF",
                        "-DCMAKE_BUILD_TYPE=Release",
                        "-DCMAKE_POLICY_VERSION_MINIMUM=3.5"
                    ],
                    "sources": [
                        {
                            "type": "archive",
                            "url": "https://gitlab.matrix.org/matrix-org/olm/-/archive/3.2.16/olm-3.2.16.tar.gz",
                            "sha256": "1e90f9891009965fd064be747616da46b232086fe270b77605ec9bda34272a68",
                            "x-checker-data": {
                                "type": "anitya",
                                "project-id": 29706,
                                "url-template": "https://gitlab.matrix.org/matrix-org/olm/-/archive/$version/olm-$version.tar.gz"
                            }
                        }
                    ],
                    "cleanup": [
                        "/include",
                        "/lib/cmake",
                        "/lib/pkgconfig"
                    ]
                }
            ]
        },
        {
            "name": "itinerary",
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "sources": [
                {
                    "type": "dir",
                    "path": "."
                }
            ],
            "config-opts": [
                "-DBUILD_TESTING=OFF",
                "-DCMAKE_DISABLE_FIND_PACKAGE_KF6KIO=ON",
                "-DCMAKE_DISABLE_FIND_PACKAGE_KF6FileMetaData=ON",
                "-DFLATPAK_BUILD=ON"
            ],
            "cleanup": [
