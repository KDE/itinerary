{
    "id": "org.kde.itinerary",
    "branch": "master",
    "runtime": "org.kde.Platform",
    "runtime-version": "6.10",
    "sdk": "org.kde.Sdk",
    "command": "itinerary",
    "tags": [
        "nightly"
    ],
    "desktop-file-name-suffix": " (Nightly)",
    "finish-args": [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri",
        "--device=all",
        "--share=network",
        "--talk-name=org.freedesktop.secrets",
        "--talk-name=org.kde.kwalletd5",
        "--talk-name=org.kde.kwalletd6"
    ],
    "modules": [
        {
            "name": "poppler",
            "config-opts": [
                "-DENABLE_TESTING=OFF",
                "-DENABLE_UNSTABLE_API_ABI_HEADERS=ON",
                "-DENABLE_BOOST=OFF",
                "-DENABLE_CPP=OFF",
                "-DENABLE_GLIB=OFF",
                "-DENABLE_GOBJECT_INTROSPECTION=OFF",
                "-DENABLE_LIBCURL=OFF",
                "-DENABLE_UTILS=OFF",
                "-DENABLE_QT5=OFF",
                "-DENABLE_QT6=OFF",
                "-DENABLE_SPLASH=OFF",
                "-DWITH_Cairo=OFF"
            ],
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "sources": [
                {
                    "type": "archive",
                    "url": "https://poppler.freedesktop.org/poppler-25.11.0.tar.xz",
                    "sha256": "63f155142b77349e2bccaef148e754e7506ab1641e713b83af4f54a8f8b15369",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 3686,
                        "stable-only": true,
                        "url-template": "https://poppler.freedesktop.org/poppler-$version.tar.xz"
                    }
                }
            ],
            "cleanup": [
                "/include",
                "/lib/pkgconfig"
            ]
        },
        {
            "name": "libphonenumer",
            "buildsystem": "cmake-ninja",
            "subdir": "cpp",
            "config-opts": [
                "-DUSE_BOOST=OFF",
                "-DBUILD_TESTING=OFF",
                "-DREGENERATE_METADATA=OFF",
                "-DBUILD_STATIC_LIB=OFF",
                "-DBUILD_SHARED_LIBS=ON",
                "-DUSE_PROTOBUF_LITE=ON",
                "-DBUILD_GEOCODER=OFF",
                "-DFETCHCONTENT_FULLY_DISCONNECTED=ON",
                "-DCMAKE_POSITION_INDEPENDENT_CODE=ON"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://github.com/google/libphonenumber/archive/refs/tags/v9.0.19.tar.gz",
                    "sha256": "ce0bf5d6154c82ae72e3ef9c90ac1fbcd93c9e9d70f0fce0e8b8e2556db3579d",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 89344,
                        "stable-only": true,
                        "url-template": "https://github.com/google/libphonenumber/archive/refs/tags/v$version.tar.gz"
                    }
                }
            ],
            "cleanup": [
                "/include",
                "/lib/*.a",
                "/lib/cmake"
            ],
            "modules": [
                {
                    "name": "protobuf",
                    "buildsystem": "cmake-ninja",
                    "config-opts": [
                        "-Dprotobuf_BUILD_TESTS=OFF",
                        "-Dprotobuf_LOCAL_DEPENDENCIES_ONLY=ON",
                        "-DBUILD_SHARED_LIBS=ON"
                    ],
                    "sources": [
                        {
                            "type": "archive",
                            "url": "https://github.com/protocolbuffers/protobuf/archive/refs/tags/v33.1.tar.gz",
                            "sha256": "0c98bb704ceb4e68c92f93907951ca3c36130bc73f87264e8c0771a80362ac97",
                            "x-checker-data": {
                                "type": "anitya",
                                "project-id": 3715,
                                "stable-only": true,
                                "url-template": "https://github.com/protocolbuffers/protobuf/archive/refs/tags/v$version.tar.gz"
                            }
                        }
                    ],
                    "cleanup": [
                        "/bin",
                        "/include",
                        "/lib/*.a",
                        "/lib/*.la",
                        "/lib/libprotoc*",
                        "/lib/cmake",
                        "/lib/pkgconfig"
                    ]
                }
            ]
        },
        {
            "name": "qtkeychain",
            "buildsystem": "cmake-ninja",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://github.com/frankosterfeld/qtkeychain/archive/0.15.0.tar.gz",
                    "sha256": "f4254dc8f0933b06d90672d683eab08ef770acd8336e44dfa030ce041dc2ca22",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 4138,
                        "stable-only": true,
                        "url-template": "https://github.com/frankosterfeld/qtkeychain/archive/$version.tar.gz"
                    }
                }
            ],
            "config-opts": [
                "-DCMAKE_INSTALL_LIBDIR=/app/lib",
                "-DLIB_INSTALL_DIR=/app/lib",
                "-DBUILD_TRANSLATIONS=NO",
                "-DBUILD_WITH_QT6=ON"
            ],
            "cleanup": [
                "/include",
                "/lib/cmake",
                "/mkspecs"
            ]
        },
        {
            "name": "kpkpass",
            "config-opts": [
                "-DBUILD_TESTING=OFF",
                "-DCMAKE_BUILD_TYPE=RelWithDebInfo"
            ],
            "buildsystem": "cmake-ninja",
            "builddir": true,
            "sources": [
                {
                    "type": "archive",
